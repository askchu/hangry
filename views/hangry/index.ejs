<% layout('layouts/boilerplate') %>

<div class="my-3">
    <h1 class="">All Locations</h1>
    <a href="hangry/results">Search locations</a>
</div>

<% for(let hangry of hangryz) { %>
<div class="card my-2">
    <div class="row">
        <div class="col-md-12 col-lg-6">
            <div class="card-body">
                <h3 class="card-title"><%=hangry.data.title%></h5>
                    <!-- <p class="card-text">Category: <%=hangry.data.entity_type%></p>
                    <p class="card-text text-muted">Entity Number: <%=hangry.data.entity_id%></p>
                    <p class="card-text">City Id: <%=hangry.data.city_id%></p> -->
                    <a href="/hangry/<%=hangry._id%>">View <%=hangry.data.title%></a>
            </div>
        </div>
    </div>
</div>
</div>
<% } %>
