<% layout('layouts/boilerplate') %>
<link rel="stylesheet" href="/stylesheets/index.css">


<div class="my-3">
    <h1 class="">All Locations</h1>
    <a href="hangry/results">Search locations</a>
</div>

<div class="text-center row">

    <% for(let hangry of hangryz) { %>
    <div class="col-md-6 col-lg-4">
        <div class="card my-2">
            <div class="card-body bg-light">
                <h3 class="card-title mb-3"><%=hangry.data.title%></h5>
                    <!-- <p class="card-text">Category: <%=hangry.data.entity_type%></p>
                    <p class="card-text text-muted">Entity Number: <%=hangry.data.entity_id%></p>
                    <p class="card-text">City Id: <%=hangry.data.city_id%></p> -->
                    <button class="btn btn-primary">
                        <a class="text-light" href="/hangry/<%=hangry._id%>">View <%=hangry.data.title%></a>
                    </button>
            </div>
        </div>
    </div>
    <% } %>
</div>
